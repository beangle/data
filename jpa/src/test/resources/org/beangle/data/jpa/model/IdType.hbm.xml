<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.beangle.data.jpa.model" default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
    <class name="org.beangle.data.jpa.model.IdType" >
        <id name="id" unsaved-value="0" type="integer">
          <generator class="auto_increment"/>
        </id>
        <property name="name" length="50" unique="true"/>
        <property name="code" length="50" unique="true"/>
    </class>
</hibernate-mapping>
